<odoo>
    <data>
        <record model="ir.cron" id="google_merchant_product">
            <field name="name">Export Google Merchant file</field>
            <field name="model_id" ref="model_product_product"/>
            <field name="state">code</field>
            <field name="code">model._create_google_merchant_product_file()</field>
            <field name="interval_number">1</field>
